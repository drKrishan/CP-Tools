{
  "name": "cptools",
  "displayName": "CP TOOLS",
  "description": "Improve Coding Efficency of CP Community",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.49.0"
  },
  "publisher": "VinothKrishanKathiresan",
  "author": {
    "name": "Vinoth Krishan Kathiresan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drKrishan/cp-snip"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
